{
  "name": "es6_setup_node",
  "version": "1.0.0",
  "description": "A simple ES6 setup for node applications",
  "main": "app.js",
  "scripts": {
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "build": "rimraf dist && babel src --out-dir dist  --copy-files",
    "start": "node dist/app.js",
    "start:dev": "nodemon --exec babel-node bin/www.js",
    "lint": "eslint .",
    "lint:fix": "eslint ./src --fix --color",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/racer242/translateEasyBot.git"
  },
  "keywords": [
    "telegram",
    "babel",
    "node.js",
    "javascript",
    "es6"
  ],
  "author": "Geeky-Geeky",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GeekyGeeky/es6_setup_node/issues"
  },
  "homepage": "https://github.com/GeekyGeeky/es6_setup_node#readme",
  "dependencies": {
    "@iamtraction/google-translate": "^1.1.2",
    "axios": "^0.27.2",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "telegraf": "^4.7.0",
    "telegraf-cmd-args": "^1.0.2",
    "telegraf-i18n": "^6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
